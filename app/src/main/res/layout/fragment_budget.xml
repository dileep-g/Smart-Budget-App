<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <TextView
            android:id="@+id/textViewProgressBar1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Progress Bar #1"
            android:textSize="15dp" />

        <ProgressBar
            android:id="@+id/progressBar1"
            style="@android:style/Widget.ProgressBar.Horizontal"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_below="@id/textViewProgressBar1"
            android:progress="75"
            android:max="150"
            android:textAlignment="center"
            android:layout_marginBottom="10dp"/>

        <TextView
        android:id="@+id/textViewProgressBar2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Progress Bar #2"
        android:textSize="15dp"
        android:layout_below="@id/progressBar1" />

        <ProgressBar
            android:id="@+id/progressBar2"
            style="@android:style/Widget.ProgressBar.Horizontal"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:progress="75"
            android:max="150"
            android:textAlignment="center"
            android:layout_below="@id/textViewProgressBar2"
            android:layout_marginBottom="10dp"/>

        <TextView
            android:id="@+id/textViewProgressBar3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Progress Bar #3"
            android:textSize="15dp"
            android:layout_below="@id/progressBar2" />

        <ProgressBar
            android:id="@+id/progressBar3"
            style="@android:style/Widget.ProgressBar.Horizontal"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:progress="75"
            android:max="150"
            android:textAlignment="center"
            android:layout_below="@id/textViewProgressBar3"
            android:layout_marginBottom="10dp"/>

        <TextView
            android:id="@+id/textViewProgressBar4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Progress Bar #4"
            android:textSize="15dp"
            android:layout_below="@id/progressBar3" />

        <ProgressBar
            android:id="@+id/progressBar4"
            style="@android:style/Widget.ProgressBar.Horizontal"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:progress="75"
            android:max="150"
            android:textAlignment="center"
            android:layout_below="@id/textViewProgressBar4"
            android:layout_marginBottom="10dp"/>

        <TextView
            android:id="@+id/textViewProgressBar5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Progress Bar #5"
            android:textSize="15dp"
            android:layout_below="@id/progressBar4" />

        <ProgressBar
            android:id="@+id/progressBar5"
            style="@android:style/Widget.ProgressBar.Horizontal"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:progress="75"
            android:max="150"
            android:textAlignment="center"
            android:layout_below="@id/textViewProgressBar5"
            android:layout_marginBottom="10dp"/>
    </RelativeLayout>


    <TextView
        android:id="@+id/text_budget"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>